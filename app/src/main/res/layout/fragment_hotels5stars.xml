<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".R_Italians">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="130dp"
            android:textAlignment="center"
            android:text="★★★★★"
            android:layout_marginTop="20dp"
            android:textSize="30dp" />

        <!-- TODO: Update blank fragment layout -->
        <androidx.cardview.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="0dp"
            app:cardBackgroundColor="@color/white"
            app:cardCornerRadius="0dp"
            app:cardElevation="0dp"
            app:cardMaxElevation="8dp"
            app:cardPreventCornerOverlap="true"
            app:cardUseCompatPadding="true">


            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <ImageView
                    android:id="@+id/idw"
                    android:layout_width="150dp"
                    android:layout_height="150dp"
                    android:layout_marginStart="5dp"
                    android:layout_marginTop="5dp"
                    android:layout_marginEnd="5dp"
                    android:layout_marginBottom="5dp"
                    android:src="@drawable/w" />

                <TextView
                    android:id="@+id/idwName"
                    android:layout_width="206dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="9dp"
                    android:layout_marginTop="10dp"
                    android:layout_toEndOf="@id/idw"
                    android:text="Hotel W"
                    android:textAlignment="center"
                    android:textColor="@color/black"
                    android:textSize="18sp"
                    android:textStyle="bold" />

                <ImageView
                    android:id="@+id/w_telf"
                    android:layout_width="45dp"
                    android:layout_height="45dp"
                    android:layout_below="@id/idwName"
                    android:layout_marginStart="31dp"
                    android:layout_marginTop="13dp"
                    android:layout_toEndOf="@id/idw"
                    android:drawablePadding="2dp"
                    android:src="@drawable/tlf" />

                <ImageView
                    android:id="@+id/w_ubi"
                    android:layout_width="45dp"
                    android:layout_height="45dp"
                    android:layout_below="@id/idwName"
                    android:layout_marginStart="85dp"
                    android:layout_marginTop="14dp"
                    android:layout_toEndOf="@id/idw"
                    android:drawablePadding="2dp"
                    android:src="@drawable/ubi" />

                <ImageView
                    android:id="@+id/w_link"
                    android:layout_width="45dp"
                    android:layout_height="45dp"
                    android:layout_below="@id/idwName"
                    android:layout_marginStart="147dp"
                    android:layout_marginTop="13dp"
                    android:layout_toEndOf="@id/idw"
                    android:drawablePadding="2dp"
                    android:src="@drawable/web" />

                <RatingBar
                    android:id="@+id/ratingw"
                    android:layout_width="235dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="160sp"
                    android:layout_marginTop="100sp" />

            </RelativeLayout>
        </androidx.cardview.widget.CardView>

        <!-- TODO: Update blank fragment layout -->
        <androidx.cardview.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="0dp"
            app:cardBackgroundColor="@color/white"
            app:cardCornerRadius="0dp"
            app:cardElevation="0dp"
            app:cardMaxElevation="8dp"
            app:cardPreventCornerOverlap="true"
            app:cardUseCompatPadding="true">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <ImageView
                    android:id="@+id/idarts"
                    android:layout_width="150dp"
                    android:layout_height="150dp"
                    android:layout_marginStart="5dp"
                    android:layout_marginTop="5dp"
                    android:layout_marginEnd="5dp"
                    android:layout_marginBottom="5dp"
                    android:src="@drawable/arts" />

                <TextView
                    android:id="@+id/idtxtarts"
                    android:layout_width="206dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="9dp"
                    android:layout_marginTop="10dp"
                    android:layout_toEndOf="@id/idarts"
                    android:text="Hotel Arts"
                    android:textAlignment="center"
                    android:textColor="@color/black"
                    android:textSize="18sp"
                    android:textStyle="bold" />

                <ImageView
                    android:id="@+id/arts_tlf"
                    android:layout_width="45dp"
                    android:layout_height="45dp"
                    android:layout_below="@id/idtxtarts"
                    android:layout_marginStart="27dp"
                    android:layout_marginTop="16dp"
                    android:layout_toEndOf="@id/idarts"
                    android:drawablePadding="2dp"
                    android:src="@drawable/tlf" />

                <ImageView
                    android:id="@+id/arts_ubi"
                    android:layout_width="45dp"
                    android:layout_height="45dp"
                    android:layout_below="@id/idtxtarts"
                    android:layout_marginStart="87dp"
                    android:layout_marginTop="16dp"
                    android:layout_toEndOf="@id/idarts"
                    android:drawablePadding="2dp"
                    android:src="@drawable/ubi" />

                <ImageView
                    android:id="@+id/arts_link"
                    android:layout_width="45dp"
                    android:layout_height="45dp"
                    android:layout_below="@id/idtxtarts"
                    android:layout_marginStart="144dp"
                    android:layout_marginTop="16dp"
                    android:layout_toEndOf="@id/idarts"
                    android:drawablePadding="2dp"
                    android:src="@drawable/web" />

                <RatingBar
                    android:id="@+id/ratingarts"
                    android:layout_width="235dp"
                    android:layout_height="40dp"
                    android:layout_alignParentEnd="true"
                    android:layout_marginLeft="160sp"
                    android:layout_marginTop="100sp"
                    android:layout_marginEnd="0dp" />

            </RelativeLayout>
        </androidx.cardview.widget.CardView>

        <!-- TODO: Update blank fragment layout -->
        <androidx.cardview.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="0dp"
            app:cardBackgroundColor="@color/white"
            app:cardCornerRadius="0dp"
            app:cardElevation="0dp"
            app:cardMaxElevation="8dp"
            app:cardPreventCornerOverlap="true"
            app:cardUseCompatPadding="true">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <ImageView
                    android:id="@+id/idMandarin"
                    android:layout_width="150dp"
                    android:layout_height="150dp"
                    android:layout_marginStart="5dp"
                    android:layout_marginTop="5dp"
                    android:layout_marginEnd="5dp"
                    android:layout_marginBottom="5dp"
                    android:src="@drawable/mandarin" />

                <TextView
                    android:id="@+id/idtxtMandarin"
                    android:layout_width="206dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="9dp"
                    android:layout_marginTop="10dp"
                    android:layout_toEndOf="@id/idMandarin"
                    android:text="Hotel Mandarin"
                    android:textAlignment="center"
                    android:textColor="@color/black"
                    android:textSize="18sp"
                    android:textStyle="bold" />

                <ImageView
                    android:id="@+id/mandarin_tlf"
                    android:layout_width="45dp"
                    android:layout_height="45dp"
                    android:layout_below="@id/idtxtMandarin"
                    android:layout_marginStart="30dp"
                    android:layout_marginTop="15dp"
                    android:layout_toEndOf="@id/idMandarin"
                    android:drawablePadding="2dp"
                    android:src="@drawable/tlf" />

                <ImageView
                    android:id="@+id/mandarin_ubi"
                    android:layout_width="45dp"
                    android:layout_height="45dp"
                    android:layout_below="@id/idtxtMandarin"
                    android:layout_marginStart="85dp"
                    android:layout_marginTop="16dp"
                    android:layout_toEndOf="@id/idMandarin"
                    android:drawablePadding="2dp"
                    android:src="@drawable/ubi" />

                <ImageView
                    android:id="@+id/mandarin_link"
                    android:layout_width="45dp"
                    android:layout_height="45dp"
                    android:layout_below="@id/idtxtMandarin"
                    android:layout_marginStart="141dp"
                    android:layout_marginTop="15dp"
                    android:layout_toEndOf="@id/idMandarin"
                    android:drawablePadding="2dp"
                    android:src="@drawable/web" />

                <RatingBar
                    android:id="@+id/ratingMandarin"
                    android:layout_width="235dp"
                    android:layout_height="45dp"
                    android:layout_marginLeft="160sp"
                    android:layout_marginTop="100sp" />

            </RelativeLayout>
        </androidx.cardview.widget.CardView>
    </LinearLayout>



</FrameLayout>