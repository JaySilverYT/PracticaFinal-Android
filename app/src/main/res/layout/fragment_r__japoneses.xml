<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".R_Japoneses">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <!-- TODO: Update blank fragment layout -->
        <androidx.cardview.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="0dp"
            app:cardBackgroundColor="@color/white"
            app:cardCornerRadius="0dp"
            app:cardElevation="0dp"
            app:cardMaxElevation="8dp"
            app:cardPreventCornerOverlap="true"
            app:cardUseCompatPadding="true">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <ImageView
                    android:id="@+id/idIVCourseImage"
                    android:layout_width="150dp"
                    android:layout_height="150dp"
                    android:layout_marginStart="5dp"
                    android:layout_marginTop="5dp"
                    android:layout_marginEnd="5dp"
                    android:layout_marginBottom="5dp"
                    android:src="@drawable/wagaya" />

                <TextView
                    android:id="@+id/idTVCourseName"
                    android:layout_width="206dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="9dp"
                    android:layout_marginTop="10dp"
                    android:layout_toEndOf="@id/idIVCourseImage"
                    android:text="Wagaya"
                    android:textAlignment="center"
                    android:textColor="@color/black"
                    android:textSize="18sp"
                    android:textStyle="bold" />

                <ImageView
                    android:id="@+id/R1_phone"
                    android:layout_width="45dp"
                    android:layout_height="45dp"
                    android:layout_below="@id/idTVCourseName"
                    android:layout_marginStart="25dp"
                    android:layout_marginTop="30dp"
                    android:layout_toEndOf="@id/idIVCourseImage"
                    android:drawablePadding="2dp"
                    android:src="@drawable/phone" />

                <ImageView
                    android:id="@+id/R1_Ubicacion"
                    android:layout_width="45dp"
                    android:layout_height="45dp"
                    android:layout_below="@id/idTVCourseName"
                    android:layout_marginStart="80dp"
                    android:layout_marginTop="30dp"
                    android:layout_toEndOf="@id/idIVCourseImage"
                    android:drawablePadding="2dp"
                    android:src="@drawable/xinxeta" />

                <ImageView
                    android:id="@+id/R1_Link"
                    android:layout_width="45dp"
                    android:layout_height="45dp"
                    android:layout_below="@id/idTVCourseName"
                    android:layout_marginStart="135dp"
                    android:layout_marginTop="30dp"
                    android:layout_toEndOf="@id/idIVCourseImage"
                    android:drawablePadding="2dp"
                    android:src="@drawable/link" />

            </RelativeLayout>
        </androidx.cardview.widget.CardView>

        <!-- TODO: Update blank fragment layout -->
        <androidx.cardview.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="0dp"
            app:cardBackgroundColor="@color/white"
            app:cardCornerRadius="0dp"
            app:cardElevation="0dp"
            app:cardMaxElevation="8dp"
            app:cardPreventCornerOverlap="true"
            app:cardUseCompatPadding="true">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <ImageView
                    android:id="@+id/idFotoItaliano"
                    android:layout_width="150dp"
                    android:layout_height="150dp"
                    android:layout_marginStart="5dp"
                    android:layout_marginTop="5dp"
                    android:layout_marginEnd="5dp"
                    android:layout_marginBottom="5dp"
                    android:src="@drawable/mifuneneko" />

                <TextView
                    android:id="@+id/idNombreItaliano"
                    android:layout_width="206dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="9dp"
                    android:layout_marginTop="10dp"
                    android:layout_toEndOf="@id/idFotoItaliano"
                    android:text="Mifune Neko"
                    android:textAlignment="center"
                    android:textColor="@color/black"
                    android:textSize="18sp"
                    android:textStyle="bold" />

                <ImageView
                    android:id="@+id/R2_phone"
                    android:layout_width="45dp"
                    android:layout_height="45dp"
                    android:layout_below="@id/idNombreItaliano"
                    android:layout_marginStart="25dp"
                    android:layout_marginTop="30dp"
                    android:layout_toEndOf="@id/idFotoItaliano"
                    android:drawablePadding="2dp"
                    android:src="@drawable/phone" />

                <ImageView
                    android:id="@+id/R2_Ubicacion"
                    android:layout_width="45dp"
                    android:layout_height="45dp"
                    android:layout_below="@id/idNombreItaliano"
                    android:layout_marginStart="80dp"
                    android:layout_marginTop="30dp"
                    android:layout_toEndOf="@id/idFotoItaliano"
                    android:drawablePadding="2dp"
                    android:src="@drawable/xinxeta" />

                <ImageView
                    android:id="@+id/R2_Link"
                    android:layout_width="45dp"
                    android:layout_height="45dp"
                    android:layout_below="@id/idNombreItaliano"
                    android:layout_marginStart="135dp"
                    android:layout_marginTop="30dp"
                    android:layout_toEndOf="@id/idFotoItaliano"
                    android:drawablePadding="2dp"
                    android:src="@drawable/link" />

            </RelativeLayout>
        </androidx.cardview.widget.CardView>

        <!-- TODO: Update blank fragment layout -->
        <androidx.cardview.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="0dp"
            app:cardBackgroundColor="@color/white"
            app:cardCornerRadius="0dp"
            app:cardElevation="0dp"
            app:cardMaxElevation="8dp"
            app:cardPreventCornerOverlap="true"
            app:cardUseCompatPadding="true">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <ImageView
                    android:id="@+id/idFotoMagrana"
                    android:layout_width="150dp"
                    android:layout_height="150dp"
                    android:layout_marginStart="5dp"
                    android:layout_marginTop="5dp"
                    android:layout_marginEnd="5dp"
                    android:layout_marginBottom="5dp"
                    android:src="@drawable/kodama" />

                <TextView
                    android:id="@+id/idNombreMagrana"
                    android:layout_width="206dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="9dp"
                    android:layout_marginTop="10dp"
                    android:layout_toEndOf="@id/idFotoMagrana"
                    android:text="Kodama"
                    android:textAlignment="center"
                    android:textColor="@color/black"
                    android:textSize="18sp"
                    android:textStyle="bold" />

                <ImageView
                    android:id="@+id/R3_phone"
                    android:layout_width="45dp"
                    android:layout_height="45dp"
                    android:layout_below="@id/idNombreMagrana"
                    android:layout_marginStart="25dp"
                    android:layout_marginTop="30dp"
                    android:layout_toEndOf="@id/idFotoMagrana"
                    android:drawablePadding="2dp"
                    android:src="@drawable/phone" />

                <ImageView
                    android:id="@+id/R3_Ubicacion"
                    android:layout_width="45dp"
                    android:layout_height="45dp"
                    android:layout_below="@id/idNombreMagrana"
                    android:layout_marginStart="80dp"
                    android:layout_marginTop="30dp"
                    android:layout_toEndOf="@id/idFotoMagrana"
                    android:drawablePadding="2dp"
                    android:src="@drawable/xinxeta" />

                <ImageView
                    android:id="@+id/R3_Link"
                    android:layout_width="45dp"
                    android:layout_height="45dp"
                    android:layout_below="@id/idNombreMagrana"
                    android:layout_marginStart="135dp"
                    android:layout_marginTop="30dp"
                    android:layout_toEndOf="@id/idFotoMagrana"
                    android:drawablePadding="2dp"
                    android:src="@drawable/link" />

            </RelativeLayout>
        </androidx.cardview.widget.CardView>
    </LinearLayout>



</FrameLayout>